<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <queries>
        <package android:name="cam.manoash.twoipcapps.appb" />
    </queries>

    <application
        android:label="App A"
        android:theme="@style/Theme.TwoIpcApps">
        <activity android:name=".MainActivityA" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".EventServiceA" android:exported="true">
            <intent-filter>
                <action android:name="cam.manoash.twoipcapps.appa.ipc.EventService" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </service>
    </application>

</manifest>
